<!-- Google Analytics -->
<% unless @brand.brand_info.google_analytics.blank? %>
  <script async src="https://www.googletagmanager.com/gtag/js?id='<%=@brand.brand_info.google_analytics %>'"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag(`js`, new Date());
    gtag(`config`, '<%= @brand.brand_info.google_analytics %>');
  </script>
<% end %>

<!-- Hubspot -->
<% unless @brand.brand_info.hubspot_tracking.blank? %>
  <script type="text/javascript" id="hs-script-loader" async defer src="//js.hs-scripts.com/<%= @brand.brand_info.hubspot_tracking %>.js"></script>
<% end %>